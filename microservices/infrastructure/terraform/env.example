# Environment variables for the Social Proof App
# Copy this file to .env and fill in the values

# Authentication Secrets
CLERK_API_KEY=sk_test_your_key
JWT_SECRET=your_jwt_secret

# Service Configuration
CORS_ALLOWED_ORIGINS=https://socialproofapp.com,https://app.socialproofapp.com,https://api.socialproofapp.com
LOG_LEVEL=info
CLERK_WEBHOOK_URL=https://api.socialproofapp.com/auth/clerk-webhook

# Token Settings
TOKEN_EXPIRY=86400
REFRESH_TOKEN_EXPIRY=2592000 
