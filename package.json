{
  "name": "social-proof-app",
  "version": "0.1.0",
  "private": true,
  "type": "commonjs",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format": "prettier --write .",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci",
    "test:unit": "jest --testPathIgnorePatterns='tests/api'",
    "test:generate-token": "node scripts/generate-token.js",
    "test:api": "jest --testPathPattern=\"app/__tests__/api\"",
    "test:e2e": "node test-e2e-flow.js",
    "test:webhook": "node test-webhook-debug.js",
    "test:fresh": "node test-fresh-flow.js",
    "test:core": "node test-core-validation.js",
    "test:db": "curl http://localhost:3007/api/debug/db-check | jq",
    "test:health": "curl http://localhost:3007/api/health",
    "deploy:staging": "./scripts/deploy-staging.sh",
    "deploy:staging:force": "./scripts/deploy-staging.sh --force",
    "deploy:check": "node scripts/check-deployment-status.js",
    "dev:all": "docker-compose -f docker-compose.dev.yml up",
    "dev:services": "docker-compose -f docker-compose.dev.yml up integrations billing notifications notification-stream users analytics",
    "dev:infra": "docker-compose -f docker-compose.dev.yml up postgres redis kafka zookeeper clickhouse",
    "dev:frontend": "docker-compose -f docker-compose.dev.yml up nextjs",
    "dev:build": "docker-compose -f docker-compose.dev.yml build",
    "dev:build:nocache": "docker-compose -f docker-compose.dev.yml build --no-cache",
    "dev:logs": "docker-compose -f docker-compose.dev.yml logs -f",
    "dev:logs:service": "docker-compose -f docker-compose.dev.yml logs -f",
    "dev:stop": "docker-compose -f docker-compose.dev.yml down",
    "dev:restart": "npm run dev:stop && npm run dev:all",
    "dev:clean": "docker-compose -f docker-compose.dev.yml down -v --remove-orphans",
    "dev:status": "docker-compose -f docker-compose.dev.yml ps",
    "dev:shell:billing": "docker-compose -f docker-compose.dev.yml exec billing sh",
    "dev:shell:integrations": "docker-compose -f docker-compose.dev.yml exec integrations sh",
    "dev:shell:notifications": "docker-compose -f docker-compose.dev.yml exec notifications sh",
    "dev:shell:postgres": "docker-compose -f docker-compose.dev.yml exec postgres psql -U postgres -d social_proof_mvp",
    "migrate": "node scripts/run-all-migrations.js",
    "migrate:dry-run": "node scripts/run-all-migrations.js --dry-run",
    "migrate:help": "node scripts/run-all-migrations.js --help",
    "migrate:billing": "node scripts/run-all-migrations.js --service=billing",
    "migrate:integrations": "node scripts/run-all-migrations.js --service=integrations",
    "migrate:analytics": "node scripts/run-all-migrations.js --service=analytics",
    "migrate:notifications": "node scripts/run-all-migrations.js --service=notifications",
    "migrate:users": "node scripts/run-all-migrations.js --service=users",
    "migrate:supabase": "echo '⚠️  Supabase migrations must be applied manually. Run: npm run migrate:help'"
  },
  "dependencies": {
    "@clerk/clerk-sdk-node": "^4.13.23",
    "@clerk/nextjs": "^6.10.2",
    "@clerk/themes": "^2.2.13",
    "@hookform/resolvers": "^3.10.0",
    "@playwright/test": "^1.52.0",
    "@radix-ui/react-accordion": "^1.2.2",
